<div class="">
    <iframe src="https://mf-map.netlify.com/map?iframe=true&staging=true&subdomain=billings" frameborder="0" width="100%" height="100%" id="iFrame1" sandbox="allow-top-navigation allow-scripts allow-same-origin"></iframe>
</div>

<script>
    $(document).ready(function(e){
        init(e);
        function renderPageData(){
            show_content();
        }
        loadMallDataCached(renderPageData);
        
    });
    window.addEventListener('message', function(event) {
        // IMPORTANT: check the origin of the data! 
        if (event.origin.startsWith('https://mf-map.netlify.com')) { 
            // The data was sent from your site.
            // Data sent with postMessage is stored in event.data:
            if(event.data && event.data.selectedStore) {
                var store = event.data.selectedStore
                window.location.href= "/stores/"+store.slug
            }
        }
    }); 
</script>
<style>
    html, iframe{
        scroll-behavior: smooth;
        height: calc(100vh - 101px) !important;
        top: 101px !important;
    }
    @media (max-width: 768px) {
        html, iframe{
            height: calc(100vh - 80px) !important;
        }
        .home_hours_phone {
            display:none!important;
        }
    }
</style>