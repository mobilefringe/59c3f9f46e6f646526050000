<script src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>
<script>
    $(document).ready(function(e){
        init(e);
        function renderAll (){
            var categories = getStoreCategories();
            var stores3 = getStoresListByCategoryID(5670);
            var stores1 = getStoresListByCategoryID(5684);

            var stores = stores1.concat(stores3).sortBy(function(o){ return o.name});
            var newStoreList = _.uniq(stores, function(o){ return o.name; });


            renderStoreList('#store_list_container','#store_list_template', newStoreList, "A", "N");
            renderStoreList('#store_list_container2','#store_list_template', newStoreList, "O", "Z");
            renderStoreList('#store_list_container_map','#store_list_template_map', newStoreList, "A", "Z");
            show_cat_stores();
            
            show_content();
        }
        
		loadMallDataCached(renderAll); 
    })
</script>